/* search/search.wxss */
.show{
  margin: 20rpx 10rpx 10rpx;
  background:#fff;
  height: 70rpx;
  border: 1px solid #f2f2f2;
  border-radius: 30rpx;
  padding-left: 50rpx;
}
.search{
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  color: #999;
}
.show image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  right: 40rpx;
  top:38rpx;
  z-index: 11
}
.fulter{
  width: 550rpx;
  height: 80rpx;
  border-bottom: 1px solid #999;
  display: flex;
  align-items: center;
  flex-direction: between;
  font-size:26rpx;
  padding-right: 80rpx;
}
.endup{
  color:#999;
  font-size: 25rpx;
}
.endup span{
  color:#79adff;
  font-size: 30rpx;
}
